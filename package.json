{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "prepare": "husky || true",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "test:update": "vitest --update",
    "test:run": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest --include 'src/**/*.test.{ts,tsx}'",
    "test:integration": "vitest --include 'src/**/*.int.test.{ts,tsx}'",
    "test:e2e": "playwright test --config=playwright.config.ts",
    "test:e2e:ui": "playwright test --config=playwright.config.ts --ui"
  },
  "dependencies": {
    "@mantine/carousel": "^8.1.1",
    "@mantine/core": "^8.1.1",
    "@mantine/dates": "^8.1.1",
    "@mantine/form": "^8.1.1",
    "@mantine/hooks": "^8.1.1",
    "@mantine/modals": "^8.1.1",
    "@mantine/notifications": "^8.1.1",
    "@mantine/nprogress": "^8.1.1",
    "@react-spring/three": "^10.0.1",
    "@react-spring/web": "^10.0.1",
    "@react-three/drei": "^10.5.1",
    "@react-three/fiber": "^9.2.0",
    "@stripe/react-stripe-js": "^3.8.0",
    "@stripe/stripe-js": "^7.6.1",
    "@tabler/icons-react": "^3.34.0",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-router": "^1.121.2",
    "@tanstack/react-router-devtools": "^1.121.5",
    "axios": "^1.9.0",
    "canvas-confetti": "^1.9.3",
    "dayjs": "^1.11.13",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "leaflet": "^1.9.4",
    "leva": "^0.10.0",
    "mantine-form-zod-resolver": "^1.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-virtuoso": "^4.13.0",
    "three": "^0.178.0",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.54.1",
    "@tanstack/router-cli": "^1.128.0",
    "@tanstack/router-plugin": "^1.121.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.178.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/eslint-plugin": "^1.2.4",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fast-check": "^3.23.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-glsl": "^1.5.1",
    "vite-plugin-restart": "^1.0.0",
    "vitest": "^3.2.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
